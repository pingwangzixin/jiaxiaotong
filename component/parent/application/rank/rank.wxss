.rank{background-color: #f5f5f5; width: 100%; height: 100%; font-family: "PingFangSC-Regular";}
.rank-head{display: flex; padding: 30rpx 42rpx;}
.head-pic{width: 80rpx; height: 80rpx;}
.head-info{flex: 1; padding-left: 24rpx;}
.head-name{display: block; font-size: 28rpx; color: #333333;}
.head-school,.head-class{color: #666666; font-size: 22rpx;}
.head-school{margin-right: 24rpx;}
.head-img{width: 83rpx;height: 90rpx;}
.rank-content{width: 706rpx;height:500rpx;  margin: auto; padding: 40rpx 60rpx; box-sizing: border-box; background: url(data:image/png;base64,) no-repeat;  background-size: cover; }
.content-font{line-height: 80rpx;display: flex;}
.font-big{font-size: 28rpx;color: #555555;}
.font-small{display: block; flex: 1; font-size: 28rpx;color: #444444;font-weight: bolder;text-align: center;}
.font-small image{width: 22rpx;height: 22rpx;}
.font-describe{font-size: 28rpx;color: #555555;font-weight: bolder;}