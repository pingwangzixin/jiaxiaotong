/* component/parent/remind/remind.wxss */
.details{background-color: #f5f5f5; width: 100%; height: 100%; font-family: "PingFangSC-Regular";}
.details-head{display: flex; padding: 30rpx 42rpx;}
.head-pic{width: 80rpx; height: 80rpx;}
.head-info{flex: 1; padding-left: 24rpx;}
.head-name{display: block; font-size: 28rpx; color: #333333;}
.head-day,.head-time{color: #666666; font-size: 22rpx;}
.head-day{margin-right: 24rpx;}
.details-content{width: 706rpx;height: 475rpx;  margin: auto; padding: 40rpx 60rpx; box-sizing: border-box; background: url(data:image/png;base64,) no-repeat;  background-size: cover; }
.content-font{line-height: 80rpx;display: flex;}
.font-big{font-size: 28rpx;color: #444444;}
.font-small{display: block; flex: 1; overflow: hidden;text-overflow:ellipsis;white-space: nowrap;width:250rpx;font-size: 24rpx;color: #444444;margin-left: 45rpx;}
.details-footer{font-size: 24rpx;color: #999999;text-align: center;margin-top: 30rpx;}