/* component/parent/msg/msg.wxss */

.msgPage {
  background: #f6f2f2;
  position: relative;
	height: 100%;
	overflow: auto;
	padding-bottom: 140rpx;
		box-sizing: border-box;
}

.userinfo-avatar {
  width: 107rpx;
  height: 107rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 4rpx #fff solid;
}

.userinfo-avatar image {
  width: 107rpx;
  height: 107rpx;
}

.msgTop {
	background: url('data:image/png;base64,') no-repeat 0 40rpx;
	background-size: contain;
	background-color: #0c7bf1;
  height: 335rpx;
  color: #fff;
  font-size: 19rpx;
  padding: 0 50rpx 0 32rpx;
	position: relative;
}

.topName {
  margin-left: 26rpx;
}

.topName>view:nth-of-type(1) {
  font-size: 28rpx;
  font-weight: 700;
  margin-bottom: 18rpx;
  margin-top: 25rpx;
}

.msgTopRight>view:nth-of-type(1) {
  margin: 32rpx 0 22rpx;
}

.msgTopRight>view {
  overflow: hidden;
}

.lateImg>image {
  width: 18rpx;
  height: 18rpx;
  margin-right: 5rpx;
}

.msgTopRight>view>view {
  float: left;
}

.msgClass {
  box-sizing: border-box;
  width: 708rpx;
  height: 420rpx;
  margin: 0 auto;
  background: #fff;
  padding: 0 58rpx;
  position: absolute;
  top: 162rpx;
  left: 0;
  right: 0;
  border-radius: 20rpx;
  font-size: 20rpx;
}

.msgClass>view:nth-of-type(1) {
  border: none;
}

.classMain {
  width: 100%;
  overflow: hidden;
  height: 123rpx;
  box-sizing: border-box;
  padding: 38rpx 10rpx 28rpx 13rpx;
  border-top: 1rpx solid #ddd;
	color: #333;
}

.classMain .className {
  color: #333;
  font-size: 27rpx;
}

.classMain .classTime {
  color: #bcbcbc;
  font-size: 18rpx;
  margin-top: 10rpx;
}

.noClass {
  text-align: center;
  color: #8a8a8a;
}

.noClassImg {
  width: 310rpx;
  height: 215rpx;
  margin: 60rpx auto 30rpx;
  font-size: 21rpx;
}

.classWrapper {
  height: 360rpx;
  overflow: hidden;
}

.classImg {
  width: 38rpx;
  height: 38rpx;
}

.classMore {
  text-align: center;
  font-size: 18rpx;
  color: #b6b6b6;
  margin: 10rpx auto;
  width: 180rpx;
  height: 25rpx;
  cursor: pointer;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 21rpx;
}

.classMore>view {
  float: left;
}

.classMore>view:nth-child(1) image {
  width: 22rpx;
  height: 22rpx;
  margin-right: 13rpx;
}

.notice {
  margin: 260rpx auto 15rpx;
  height: 60rpx;
  font-size: 22rpx;
  color: #333;
  background: #fff;
  width: 708rpx;
  border-radius: 20rpx;
  line-height: 60rpx;
  padding-left: 50rpx;
  padding-right: 29rpx;
  box-sizing: border-box;
}

.noticeImg {
  width: 33rpx;
  height: 33rpx;
  margin: 13rpx 12rpx 0 0;
}

.noticeContent {
  width: 510rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.homeWork {
  box-sizing: border-box;
  width: 708rpx;
  border-radius: 20rpx;
  background: #fff;
  margin: auto;
  padding: 0 50rpx;
}

.workTitle {
  height: 72rpx;
  line-height: 72rpx;
  font-size: 28rpx;
  color: #333;
}

.workMain {
  height: 157rpx;
  border-top: 1rpx solid #ddd;
  padding-top: 33rpx;
}

.workLeft {
  height: 84rpx;
  width: 72rpx;
  margin-left: 16rpx;
  margin-right: 58rpx;
}

.workLeft image {
  height: 84rpx;
  width: 72rpx;
}

.workT {
  font-size: 25rpx;
  color: #333;
}

.workDec {
  font-size: 20rpx;
  color: #666;
  margin: 5rpx 0;
}

.workTime {
  font-size: 20rpx;
  color: #666;
}
